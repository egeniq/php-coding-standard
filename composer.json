  "description": "The Egeniq Coding Standard is a set of PHPCS rules that we use at Egeniq, it's based on the Doctrine project.",
  "keywords": [
    "coding",
    "standard",
    "cs",
    "code",
    "style",
    "sniffer",
    "rules",
    "sniffs",
    "checks"
  ],
  "license": "MIT",
  "authors": [],
  "require": {
    "php": "^7.1",
    "dealerdirect/phpcodesniffer-composer-installer": "^0.5.0",
    "slevomat/coding-standard": "~5.0",
    "squizlabs/php_codesniffer": "^3.4.0"
  },
  "config": {
    "sort-packages": true
  }
}
