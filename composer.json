{
  "name": "egeniq/php-coding-standard",
  "type": "phpcodesniffer-standard",
  "description": "The Egeniq Coding Standard is a set of PHPCS rules that we use at Egeniq, it's based on the Doctrine project.",
  "keywords": [
    "coding",
    "standard",
    "cs",
    "code",
    "style",
    "sniffer",
    "rules",
    "sniffs",
    "checks"
  ],
  "license": "MIT",
  "authors": [],
  "autoload": {
    "psr-4": {
      "Egeniq\\Sniffs\\": "lib/Egeniq/Sniffs"
    }
  },
  "require": {
    "php": ">=7.4",
    "dealerdirect/phpcodesniffer-composer-installer": "^0.7",
    "slevomat/coding-standard": "^8.0",
    "squizlabs/php_codesniffer": "^3.4"
  },
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  }
}
